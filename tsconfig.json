{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": "./",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "src": [
        "./src"
      ],
      "components/*": [
        "./src/components/*"
      ],
      "components": [
        "./src/components"
      ],
      "contexts/*": [
        "./src/contexts/*"
      ],
      "contexts": [
        "./src/contexts"
      ],
      "data/*": [
        "./src/data/*"
      ],
      "data": [
        "./src/data"
      ],
      "pages": [
        "./src/pages"
      ],
      "pages/*": [
        "./src/pages/*"
      ],
      "hooks/*": [
        "./src/hooks/*"
      ],
      "hooks": [
        "./src/hooks"
      ],
    }
  },
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "node_modules",
  ]
}